import java.util.Scanner;
class Power	{
  public static void main(String[] args){
  System.out.print("enter the number: ");
  Scanner sc=new Scanner(System.in);
  int n=sc.nextInt(); 
  System.out.print("enter the expoential: ");
  int e=sc.nextInt();
  int res=1;
  while (e!=0){
	  res=res*n;
	  e--;
	  }
  System.out.print("result is "+res);
  }
}
