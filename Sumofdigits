import  java.util.*;
class Sumofdigits{
public static void main(String[] args){
	System.out.print("enter a number");
	Scanner sc=new Scanner(System.in);
	int n=sc.nextInt();
	int sum=0;
	int rem=0;
	while (n>0){
		rem=n%10;
		sum=sum+rem;
		n=n/10;
		}
		System.out.print("sum of digits is"+sum);
	}
}		
